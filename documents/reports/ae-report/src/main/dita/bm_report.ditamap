<!DOCTYPE bookmap PUBLIC "-//OASIS//DTD DITA BookMap//EN" "http://docs.oasis-open.org/dita/v1.1/OS/dtd/bookmap.dtd">
<bookmap id="dcc-bill-of-material">
    <title>
        <ph>${product.name} ${product.version}</ph>
    </title>
    <bookmeta>
        <category>Vulnerability Report</category>
        <bookid>
            <bookpartno>${document.id}</bookpartno>
        </bookid>
        <bookrights>
