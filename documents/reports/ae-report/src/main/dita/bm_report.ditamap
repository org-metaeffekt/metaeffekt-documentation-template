        <topicref href="example-report/gen/tpc_inventory-vulnerability-details.dita" />
    </chapter>

    <backmatter>
        <booklists>
            <tablelist />
            <glossarylist>
                <topicref format="ditamap" href="example-report/gen/glossary/en/gmap_glossary.ditamap" />
            </glossarylist>
        </booklists>
    </backmatter>

</bookmap>
